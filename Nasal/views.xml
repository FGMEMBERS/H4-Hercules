<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-100.0</chase-distance-m>

  <view n="100">
    <name>Copilot View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.593 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   6.498 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -13.600 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg>0</pitch-offset-deg>
      <heading-offset-deg type="double">0</heading-offset-deg> <!-- inside its window -->
      <default-field-of-view-deg>70</default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Enginer View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  1.155 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  5.525 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y">-12.325 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg>-4</pitch-offset-deg>
      <heading-offset-deg type="double">270</heading-offset-deg> <!-- parallel to the panel -->
      <default-field-of-view-deg>50</default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>Radio View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  -0.922 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   5.525 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -12.291 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg>-4</pitch-offset-deg>
      <heading-offset-deg type="double">90</heading-offset-deg> <!-- parallel to the panel -->
      <default-field-of-view-deg>50</default-field-of-view-deg>
    </config>
  </view>

  <view n="103">
    <name>Star View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.00 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  6.329 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y">  0.340 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg>0</pitch-offset-deg>
      <heading-offset-deg type="double">0</heading-offset-deg> <!-- towards the stern -->
      <default-field-of-view-deg>50</default-field-of-view-deg>
    </config>
  </view>

  <view n="104">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 21.777 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 39.163 </z-offset-m> <!--Back-->
      <default-field-of-view-deg>50</default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <name>Pilot View</name>
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.528 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   6.337 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -13.889 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg>-15</pitch-offset-deg>
      <default-field-of-view-deg>70</default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

</PropertyList>
