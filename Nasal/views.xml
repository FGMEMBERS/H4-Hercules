<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -100 </chase-distance-m>

  <view n="100">
    <name>Copilot View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.58556 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   6.35096 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -13.68172 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg type="double"> 0 </heading-offset-deg> <!-- inside its window -->
      <default-field-of-view-deg> 70 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Enginer View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  1.22676 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  5.62667 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y">-12.34087 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <heading-offset-deg type="double"> 270 </heading-offset-deg> <!-- parallel to the panel -->
      <default-field-of-view-deg> 55 </default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>Radio View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  -1.34370 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   5.62667 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -12.29402 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <heading-offset-deg type="double"> 90 </heading-offset-deg> <!-- parallel to the panel -->
      <default-field-of-view-deg> 55 </default-field-of-view-deg>
    </config>
  </view>

  <view n="103">
    <name>Star View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.00 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  6.329 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y">  0.340 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <heading-offset-deg type="double"> 0 </heading-offset-deg> <!-- towards the stern -->
      <default-field-of-view-deg> 50 </default-field-of-view-deg>
    </config>
  </view>

  <view n="104">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 21.777 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 39.163 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 50 </default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <name>Pilot View</name>
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.58556 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">   6.35096 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -13.68172 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -15 </pitch-offset-deg>
      <default-field-of-view-deg> 70 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>

