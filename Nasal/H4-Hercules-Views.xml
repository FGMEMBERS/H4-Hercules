<?xml version="1.0"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-100.00</chase-distance-m>


  <!-- pilot seat in cockpit. CTRL V -->

  <view>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">55.0</default-field-of-view-deg>
      <default-pitch-deg type="double">0</default-pitch-deg>
      <default-heading-deg type="double">0</default-heading-deg>
      <front-direction-deg type="double">0</front-direction-deg>
      <front-left-direction-deg type="double">45</front-left-direction-deg>
      <left-direction-deg type="double">90</left-direction-deg>
      <back-left-direction-deg type="double">135</back-left-direction-deg>
      <back-direction-deg type="double">180</back-direction-deg>
      <back-right-direction-deg type="double">225</back-right-direction-deg>
      <right-direction-deg type="double">270</right-direction-deg>
      <front-right-direction-deg type="double">315</front-right-direction-deg>
      <!-- x/y/z == right/up/back -->
      <x-offset-m archive="y">  -0.528 </x-offset-m>
      <y-offset-m archive="y">   4.337 </y-offset-m>
      <z-offset-m archive="y"> -13.889 </z-offset-m>
      <pitch-offset-deg>-15</pitch-offset-deg>
    </config>
  </view>

  <!-- additional seats. -->

  <current-view>
    <seat>
      <captain type="bool">true</captain>
      <engineer type="bool">false</engineer>
      <radio type="bool">false</radio>
      <copilot type="bool">false</copilot>
      <star type="bool">false</star>
    </seat>
  </current-view>

  <view n="100">
    <name>Engineer View</name>
    <type>lookfrom</type>
    <internal type="bool">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">55.0</default-field-of-view-deg>
      <default-pitch-deg type="double">0</default-pitch-deg>
      <default-heading-deg type="double">0</default-heading-deg>
      <front-direction-deg type="double">0</front-direction-deg>
      <front-left-direction-deg type="double">45</front-left-direction-deg>
      <left-direction-deg type="double">90</left-direction-deg>
      <back-left-direction-deg type="double">135</back-left-direction-deg>
      <back-direction-deg type="double">180</back-direction-deg>
      <back-right-direction-deg type="double">225</back-right-direction-deg>
      <right-direction-deg type="double">270</right-direction-deg>
      <front-right-direction-deg type="double">315</front-right-direction-deg>
      <!-- x/y/z == right/up/back CTRL E -->
      <x-offset-m archive="y">  1.155 </x-offset-m>
      <y-offset-m archive="y">  3.525 </y-offset-m> <!-- head below the window -->
      <z-offset-m archive="y">-12.325 </z-offset-m> <!-- behind the rear windows, before the wing door -->
      <pitch-offset-deg>-4</pitch-offset-deg>
      <heading-offset-deg type="double">270</heading-offset-deg> <!-- parallel to the panel -->
    </config>
  </view>

  <view n="101">
    <name>Radio View</name>
    <type>lookfrom</type>
    <internal type="bool">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">55.0</default-field-of-view-deg>
      <default-pitch-deg type="double">0</default-pitch-deg>
      <default-heading-deg type="double">0</default-heading-deg>
      <front-direction-deg type="double">0</front-direction-deg>
      <front-left-direction-deg type="double">45</front-left-direction-deg>
      <left-direction-deg type="double">90</left-direction-deg>
      <back-left-direction-deg type="double">135</back-left-direction-deg>
      <back-direction-deg type="double">180</back-direction-deg>
      <back-right-direction-deg type="double">225</back-right-direction-deg>
      <right-direction-deg type="double">270</right-direction-deg>
      <front-right-direction-deg type="double">315</front-right-direction-deg>
      <!-- x/y/z == right/up/back CTRL O -->
      <x-offset-m archive="y">  -0.922 </x-offset-m>
      <y-offset-m archive="y">   3.525 </y-offset-m> <!-- head below the window -->
      <z-offset-m archive="y"> -12.291 </z-offset-m> <!-- fowards rear window -->
      <pitch-offset-deg>-4</pitch-offset-deg>
      <heading-offset-deg type="double">90</heading-offset-deg> <!-- parallel to the panel -->
    </config>
  </view>

  <view n="102">
    <name>Copilot View</name>
    <type>lookfrom</type>
    <internal type="bool">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">55.0</default-field-of-view-deg>
      <default-pitch-deg type="double">0</default-pitch-deg>
      <default-heading-deg type="double">0</default-heading-deg>
      <front-direction-deg type="double">0</front-direction-deg>
      <front-left-direction-deg type="double">45</front-left-direction-deg>
      <left-direction-deg type="double">90</left-direction-deg>
      <back-left-direction-deg type="double">135</back-left-direction-deg>
      <back-direction-deg type="double">180</back-direction-deg>
      <back-right-direction-deg type="double">225</back-right-direction-deg>
      <right-direction-deg type="double">270</right-direction-deg>
      <front-right-direction-deg type="double">315</front-right-direction-deg>
      <!-- x/y/z == right/up/back CTRL J -->
      <x-offset-m archive="y">   0.593 </x-offset-m> <!-- symmetry with captain -->
      <y-offset-m archive="y">   4.498 </y-offset-m>
      <z-offset-m archive="y"> -13.600 </z-offset-m>
      <pitch-offset-deg>0</pitch-offset-deg>
      <heading-offset-deg type="double">0</heading-offset-deg> <!-- inside its window -->
    </config>
  </view>

  <view n="103">
    <name>Star View</name>
    <type>lookfrom</type>
    <internal type="bool">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">55.0</default-field-of-view-deg>
      <default-pitch-deg type="double">0</default-pitch-deg>
      <default-heading-deg type="double">0</default-heading-deg>
      <front-direction-deg type="double">0</front-direction-deg>
      <front-left-direction-deg type="double">45</front-left-direction-deg>
      <left-direction-deg type="double">90</left-direction-deg>
      <back-left-direction-deg type="double">135</back-left-direction-deg>
      <back-direction-deg type="double">180</back-direction-deg>
      <back-right-direction-deg type="double">225</back-right-direction-deg>
      <right-direction-deg type="double">270</right-direction-deg>
      <front-right-direction-deg type="double">315</front-right-direction-deg>
      <!-- x/y/z == right/up/back -->
      <x-offset-m archive="y">   0.00 </x-offset-m> <!-- centered -->
      <y-offset-m archive="y">  4.329 </y-offset-m>
      <z-offset-m archive="y">  0.340 </z-offset-m>
      <pitch-offset-deg>0</pitch-offset-deg>
      <heading-offset-deg type="double">0</heading-offset-deg> <!-- towards the stern -->
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double">3.49</target-z-offset-m>
    </config>
  </view>

</PropertyList>
