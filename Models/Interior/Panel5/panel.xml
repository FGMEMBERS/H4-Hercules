<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Hughes H4 Hercules - 1946 -->

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <object-name>tableau</object-name>
    <object-name>manette1-ing</object-name>
    <object-name>manette2-ing</object-name>
    <object-name>manette3-ing</object-name>
    <object-name>manette4-ing</object-name>
    <object-name>manette5-ing</object-name>
    <object-name>manette6-ing</object-name>
    <object-name>manette7-ing</object-name>
    <object-name>manette8-ing</object-name>
    <object-name>bouton1</object-name>
    <object-name>bouton2</object-name>
    <object-name>bouton3</object-name>
    <object-name>bouton4</object-name>
    <object-name>bouton5</object-name>
    <object-name>bouton6</object-name>
    <object-name>bouton7</object-name>
    <object-name>bouton8</object-name>

    <!-- transparent objects -->
  </animation>

  <!-- Zones de clique souris -->
  <model>
    <path>Aircraft/H4-Hercules/Models/Interior/Panel5/Mouse/zone.xml</path>
  </model>

  <!-- Les manettes des gaz -->
  <animation>
    <type>rotate</type>
    <object-name>manette1-ing</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.068 </y-m>
      <z-m> -0.111 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

