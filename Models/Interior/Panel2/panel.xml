    <object-name>bouton2</object-name>
    <object-name>bouton3</object-name>
    <object-name>bouton4</object-name>
    <object-name>bouton5</object-name>
    <object-name>trous</object-name>

    <!-- transparent objects -->
  </animation>

  <!-- Zones de clique souris -->
  <model>
    <path>Aircraft/H4-Hercules/Models/Interior/Panel2/Mouse/zone.xml</path>
  </model>

  <!-- Manettes des gaz N°1 (pilote et copilote) -->
  <animation>
    <type>rotate</type>
    <object-name>manette1-pilote</object-name>
    <object-name>manette1-copilote</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> 80 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
