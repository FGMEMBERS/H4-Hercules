<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Hughes H4 Hercules - 1946 -->
<PropertyList>

  <path>panel.ac</path>

  <animation>
    <object-name>manettes</object-name>
    <object-name>tableau6</object-name>
    <object-name>tableau7</object-name>
    <object-name>soustableau7</object-name>
