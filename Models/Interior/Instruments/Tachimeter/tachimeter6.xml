<?xml version="1.0"?>
<PropertyList>

 <path>tachimeter.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>/engines/engine[5]/rpm</property>
  <min>0</min>
  <max>4500</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>4500</ind>
    <dep>330</dep>
   </entry>
  </interpolation>
  <axis>
   <x> -1.000 </x>
   <y>  0.000 </y>
   <z>  0.000 </z>
  </axis>
 </animation>

</PropertyList>


