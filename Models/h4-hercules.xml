<!-- Animations des hélices -->

<!-- Sélection/déselection en fonction des rpm moteurs -->
<!-- On sélectionne le disque si rpm supérieur ou égale 900 -->
 <animation>
   <type>select</type> 
   <object-name>disque-D1</object-name>
   <condition>
     <greater-than>
       <property>/engines/engine[0]/rpm</property>
       <value>1750</value>
     </greater-than>
   </condition>
 </animation>

